    PostalCode: safeGet(msg, ['PID', 'PID.11', 'PID.11.5'], null),
    Country: safeGet(msg, ['PID', 'PID.11', 'PID.11.6'], 'US')
};

// Store patientKey in channelMap for use in Encounter destination
channelMap.put('patientPayload', JSON.stringify(patientPayload));

return JSON.stringify(patientPayload, null, 2);</script>
