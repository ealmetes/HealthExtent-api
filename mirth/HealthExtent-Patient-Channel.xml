            <script>// Build Patient JSON payload with null safety
function safeGet(obj, path, defaultVal) {
    try {
        var result = obj;
        for each (var key in path) {
            if (result &amp;&amp; result[key] !== undefined) {
                result = result[key];
            } else {
                return defaultVal;
            }
        }
        return result ? result.toString() : defaultVal;
    } catch (e) {
        return defaultVal;
    }
}

var patientPayload = {
    TenantKey: parseInt($g('tenantKey')),
    PatientIdExternal: safeGet(msg, ['PID', 'PID.3', 'PID.3.1'], null),
    AssigningAuthority: safeGet(msg, ['PID', 'PID.3', 'PID.3.4'], 'DEFAULT'),
    MRN: safeGet(msg, ['PID', 'PID.2', 'PID.2.1'], null) || safeGet(msg, ['PID', 'PID.3', 'PID.3.1'], null),
    FamilyName: safeGet(msg, ['PID', 'PID.5', 'PID.5.1'], null),
    GivenName: safeGet(msg, ['PID', 'PID.5', 'PID.5.2'], null),
    MiddleName: safeGet(msg, ['PID', 'PID.5', 'PID.5.3'], null),
    DOB_TS: safeGet(msg, ['PID', 'PID.7', 'PID.7.1'], null),
    Sex: safeGet(msg, ['PID', 'PID.8'], null),
    SSN: safeGet(msg, ['PID', 'PID.19'], null),
    Phone: safeGet(msg, ['PID', 'PID.13', 'PID.13.1'], null),
    Email: safeGet(msg, ['PID', 'PID.13', 'PID.13.4'], null),
    AddressLine1: safeGet(msg, ['PID', 'PID.11', 'PID.11.1'], null),
